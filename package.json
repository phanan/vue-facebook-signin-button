{
  "name": "vue-facebook-signin-button",
  "version": "1.0.1",
  "description": "A simple plugin to include a custom Facebook sign-in button into your web app",
  "main": "dist/vue-facebook-signin-button.min.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint index.js",
    "build": "./node_modules/.bin/eslint index.js && BABEL_ENV=production && babel index.js -o ./dist/vue-facebook-signin-button.min.js"
  },
  "eslintConfig": {
    "extends": "vue"
  },
  "babel": {
    "presets": [
      "es2015",
      "babili"
    ]
  },
  "keywords": [
    "Vue",
    "facebook",
    "oAuth",
    "sign-in",
    "sign in",
    "log-in",
    "log in"
  ],
  "author": "Phan An <me@phanan.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phanan/vue-facebook-signin-button/"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-babili": "0.0.9",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^4.18.2",
    "eslint-config-vue": "^2.0.1",
    "eslint-plugin-vue": "^1.0.0"
  }
}
